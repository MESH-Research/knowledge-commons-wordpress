    "aws/aws-sdk-php": "^3.273"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "allow-plugins": {
      "composer/installers": true,
      "roots/wordpress-core-installer": true,
      "simplesamlphp/composer-module-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "site/web/app/mu-plugins/{$name}/": [
        "type:wordpress-muplugin",
        "wpackagist-plugin/wp-saml-auth"
      ],
      "site/web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "site/web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "site/web/wp"
  },
  "scripts": {
    "test": [
      "phpcs"
    ]
